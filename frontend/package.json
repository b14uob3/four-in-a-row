{
    "name": "@solana/wallet-adapter-react-ui-starter",
    "version": "0.9.18",
    "author": "Solana Maintainers <maintainers@solana.foundation>",
    "repository": "https://github.com/solana-labs/wallet-adapter",
    "license": "Apache-2.0",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "src",
        ".editorconfig",
        ".env",
        ".gitignore",
        ".prettierignore",
        ".prettierrc",
        "LICENSE",
        "package.json",
        "tsconfig.json"
    ],
    "scripts": {
